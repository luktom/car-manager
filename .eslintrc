{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": [0],
    "object-curly-newline": ["error", {
      "ObjectPattern": { "multiline": false }
    }],
    "arrow-parens": [2, "as-needed"],
    "no-underscore-dangle": [0]
  }
}